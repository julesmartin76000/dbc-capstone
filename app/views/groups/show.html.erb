<h3>Welcome to the group : <%= @group.location%></h3>
<p>Here is the list of avaible walk requests</p>

<div class="col s12 m6">
  <h2 class="header"></h2>
  <div class="card horizontal">

		<div class="card-stacked">
			<ul>
				<% @walks.whereeach do |walk|%>
  				<div id="cards-content">
    				<li><%= walk.starting_location %></li>
    				<li><%= walk.requester.username %></li>
    				<li><%= walk.destination %></li>
    				<li><%= walk.walk_time %></li>
    				<%= link_to '+', {:controller => "rooms", :action => "show" } %>
  <!--  I think this link actually needs to link hit the update for the walk with this ID and set the guar-->
  				</div>
			<% end %>
			</ul>
	</div>
</div>
