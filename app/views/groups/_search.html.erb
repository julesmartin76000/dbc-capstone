<div class="row">
  <div class="col s12">
    <div class="row">
			<%= form_tag(groups_path, :method => "get") do %>
				<div class="input-field col s12">
				  <%= text_field_tag :group_title, params[:group_title], placeholder: "Search Groups" %>
				  <%= submit_tag "Search", type: "hidden" %>
				</div>   
			<% end %>
		</div>
  </div>